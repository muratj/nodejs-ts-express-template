{
  "name": "nodejs-ts-express-template",
  "version": "1.0.0",
  "description": "NodeJS TypeScript Express starter template",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=prod node ./dist/index.js",
    "dev": "NODE_ENV=dev nodemon ./src/index.ts",
